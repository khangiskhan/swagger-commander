{
	"preset": "crockford",
	"requireMultipleVarDecl": null,
	"requireVarDeclFirst": null,
	"disallowDanglingUnderscores": null,
	"maximumLineLength": 120,
	"requireSpaceAfterKeywords": [
		"do",
		"for",
		"if",
		"else",
		"switch",
		"case",
		"try",
		"catch",
		"void",
		"while",
		"with",
		"return",
		"typeof"
	],
	"excludeFiles": [
		"nodemon",
		"coverage",
        "api-ui",
        "spec"
	]
}
